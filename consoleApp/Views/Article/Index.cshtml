@model List<ConsoleApp.Models.ArticleViewModel>
@{
    Layout = "_Layout";
}
<h1>@Model.Count</h1>

@if (Model.Count > 0)
{
    <h4>Registered Logins</h4>
    <table class="table">
        <tbody>
            @for (var index = 0; index < Model.Count; index++)
            {
                <tr>
                    <td><a target="_blank" href="Article/Art/@Model[index].ArtID">@Model[index].ArtTitle </a></td>
                    <td><a target="_blank" href="Article/Art/@Model[index].ArtID">@Model[index].ArtDetail</a></td>
                    <td>@Model[index].UserName</td>
                    <td>@Model[index].EditTime</td>
                    <td>@Model[index].ArtTypeName</td>
                </tr>
            }
        </tbody>
    </table>
}
