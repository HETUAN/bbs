@model List<ConsoleApp.Models.UserViewModel>
@{
    Layout = "_Layout";
}
<h1>@Model.Count</h1>

@if (Model.Count > 0)
{
    <h4>Registered Logins</h4>
    <table class="table">
        <tbody>
            @for (var index = 0; index < Model.Count; index++)
            {
                <tr>
                    <td>@Model[index].UserName</td>
                    <td>@Model[index].Mail</td>
                </tr>
            }
        </tbody>
    </table>
}
